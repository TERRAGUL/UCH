<Window x:Class="p3265.vtoroeokno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:p3265"
        mc:Ignorable="d"
        Title="vtoroeokno" Height="450" Width="800">
    <Grid>
        <DataGrid
    x:Name="vtorayatabl"
    AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Имя студента" Binding="{Binding Students.Nameee}" />
                <DataGridTextColumn Header="Возраст студента" Binding="{Binding Students.Age}" />
                <DataGridTextColumn Header="Предмет" Binding="{Binding Students.Major}" />
                <DataGridTextColumn Header="Наименование курса" Binding="{Binding CourseName}" />
                <DataGridTextColumn Header="Инструктор" Binding="{Binding Instructor}" />
                <DataGridTextColumn Header="Оценка" Binding="{Binding Credits}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
